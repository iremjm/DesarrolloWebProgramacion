import java.util.Scanner;

/*7. Realizar un programa que pida las notas de 10 alumnos por pantalla y muestre un
		menú de opciones: 1. Listar notas, 2.Calcular la media, 3.Calcular el menor, calcular
		el mayor.*/
public class Ejercicio7 {

	public static void main(String[] args) {
		Scanner tc=new Scanner(System.in);
		double notaBBDD;
		double notaLM;
		double SI;
		System.out.println("Ingrese la nota de BBDD: ");
		notaBBDD=tc.nextDouble();
		System.out.println("Ingrese la nota de LM: ");
		notaLM=tc.nextDouble();
		System.out.println("Ingrese la nota de SI: ");
		notaBBDD=tc.nextDouble();
	}

}
