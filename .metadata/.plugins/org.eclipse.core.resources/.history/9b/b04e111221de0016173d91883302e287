package ExamenRecu;

import ExamenRecu.Ejercicio4.Oveja;

public class Ejercicio5 {

	public static void main(String[] args) {
		Oveja o1=new Oveja();
		Oveja o2=new Oveja();
		Oveja o3=new Oveja();
		o1.setNombre("Juanita");
		o2.setNombre("Peter");
		o3.setNombre("Ricardin");
		o3.setMadre(o1);
		o3.setPadre(o2);
		System.out.println("Ingrea el crotal");
		
		System.out.println(o3.toString());
	}

}
