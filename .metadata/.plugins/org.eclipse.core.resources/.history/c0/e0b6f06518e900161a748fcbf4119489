package Banco;

public class Programa {

	public static void main(String[] args) {
		//Cuenta Credito
		CuentaCredito c1= new CuentaCredito("Juan",900);
		c1.reintegro(1000);
		System.out.println(c1.getTitular()+" tu saldo es: "+c1.getSaldo());
		System.out.println("-------------------------------------------------");
		//Cuenta Nomina
		CuentaNomina c2=new CuentaNomina("Pepe",1000);
		c2.reintegro(2000);
		System.out.print("\n"+c2.getTitular()+" tu saldo es: "+c2.getSaldo());
		
	}
	

}
